{
  "name": "tweet-your-bracket",
  "version": "3.1.0",
  "author": {
    "name": "Luke Karrys",
    "email": "luke@lukekarrys.com",
    "url": "http://lukekarrys.com"
  },
  "bugs": {
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com/issues"
  },
  "dependencies": {
    "backbone": "^1.1.2",
    "bracket-data": "^2.0.6",
    "bracket-data-live": "git+https://github.com/tweetyourbracket/bracket-data-live.git",
    "bracket-generator": "^2.0.2",
    "bracket-scorer": "^2.0.0",
    "bracket-updater": "^2.0.1",
    "bracket-validator": "^2.0.2",
    "express": "3.x.x",
    "fastclick": "^1.0.0",
    "favicon-setter": "~0.1.1",
    "figs": "^0.1.4",
    "human-model": "~2.6.1",
    "human-view": "git+https://github.com/lukekarrys/human-view#render-collection-fix",
    "human-view-switcher": "~0.0.1",
    "jade": "~1.2.0",
    "jquery": "~2.1.0",
    "less": "~1.6.3",
    "lessitizer": "0.0.1",
    "moment": "^2.5.1",
    "moonboots-express": "^0.1.0",
    "moonboots-static": "0.0.4",
    "node-uuid": "^1.4.1",
    "simplewebrtc": "~1.1.1",
    "underscore": "1.5.2"
  },
  "devDependencies": {
    "divshot-cli": "^0.7.0",
    "nodemon": "~1.0.14",
    "precommit-hook": "~0.3.8",
    "spa-crawler": "0.0.1",
    "templatizer": "~0.2.10",
    "mkdirp": "^0.4.0",
    "execSync": "^1.0.1-pre"
  },
  "homepage": "http://tweetyourbracket.com",
  "jshintConfig": {
    "asi": false,
    "browser": true,
    "curly": false,
    "expr": true,
    "indent": 4,
    "loopfunc": true,
    "node": true,
    "predef": [
      "$",
      "app",
      "me"
    ],
    "trailing": true,
    "undef": true,
    "unused": true,
    "white": true
  },
  "keywords": [
    "ncaa",
    "bracket",
    "tweetyourbracket",
    "scores"
  ],
  "license": "MIT",
  "main": "server.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com.git"
  },
  "scripts": {
    "local": "./node_modules/.bin/nodemon -e js,json,jade server --local",
    "production": "./node_modules/.bin/divshot push production",
    "update-data": "npm update bracket-data-live",
    "build": "node server --build",
    "pages": "node server --pages",
    "build-prod": "npm run update-data && npm run build && npm run production",
    "render": "phantomjs ./node_modules/rndr-me/server.js --port=8001 --readyEvent=renderReady"
  }
}
