  "main": "server.js",
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com.git"
  },
  "scripts": {
    "build": "npm run webpack-config && TYB_STATIC=true NODE_ENV=production webpack --progress",
    "codeship-setup": "npm run npm-version && npm install && npm run build",
    "codeship-test": "npm test && npm run lint",
    "deploy": "divshot",
    "lint": "eslint --ext .js,.jsx,.es6 .",
    "npm-version": "npm install -g npm@`cat package.json | jq -r '.engines.npm'`",
    "postbuild": "npm run size",
    "postinstall": "rm .validate.json .jshint{rc,ignore} &>/dev/null; exit 0",
    "prebuild": "rm -rf build && mkdir build",
    "size": "for file in build/*; do echo `cat $file | gzip-size | pretty-bytes` $file; done;",
    "start": "npm run webpack-config && TYB_STATIC=true webpack-dev-server --progress",
    "test": "babel-tape-runner test/*.js | tap-spec",
    "validate": "npm ls >/dev/null",
    "webpack-config": "babel webpack.config.es6 --out-file webpack.config.js"
  }
}
