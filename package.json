    "bracket-data": "^2.2.9",
    "bracket-generator": "^2.0.4",
    "bracket-scorer": "^2.3.2",
    "bracket-updater": "^2.0.4",
    "bracket-validator": "^2.0.4",
    "firebase": "^2.2.3",
    "lesshat": "^3.0.2",
    "lodash": "^3.5.0",
    "qs": "^2.4.1",
    "raf": "^2.0.4",
    "react": "^0.12.2",
    "react-bootstrap": "^0.17.0",
    "react-router": "^0.12.4",
    "react-router-bootstrap": "^0.10.0",
    "react-timeago": "^2.0.2",
    "xhr": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^4.7.16",
    "babel-loader": "^4.2.0",
    "babel-tape-runner": "^1.0.0",
    "css-loader": "^0.9.1",
    "divshot-cli": "^1.10.3",
    "extract-text-webpack-plugin": "^0.4.0",
    "gzip-size": "^1.0.0",
    "html-webpack-plugin": "^1.1.0",
    "json-loader": "^0.5.1",
    "jsxhint": "^0.13.2",
    "less": "^2.4.0",
    "less-import-inserter": "^1.0.1",
    "less-loader": "^2.1.0",
    "precommit-hook": "^1.0.7",
    "pretty-bytes": "^1.0.3",
    "react-hot-loader": "^1.2.3",
    "style-loader": "^0.8.3",
    "tap-spec": "^2.2.2",
    "tape": "^3.5.0",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.7.0"
  },
  "engines": {
    "node": "^0.10.35",
    "npm": "^2.7.0"
  },
  "homepage": "http://tweetyourbracket.com",
  "keywords": [
    "bracket",
    "ncaa",
    "tweetyourbracket"
  ],
  "license": "MIT",
  "main": "server.js",
  "pre-commit": [
    "lint",
    "test"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com.git"
  },
  "scripts": {
    "build": "rm -rf build/ && NODE_ENV=production webpack && npm run build-after",
    "build-after": "mkdir -p build/assets/ && mv build/bundle.* build/assets && rsync -rv --exclude=*.html public/ build/",
    "codeship-setup": "npm run npm-version && npm install && npm run build",
    "codeship-test": "npm test && npm run lint && npm run size",
    "deploy": "divshot",
    "lint": "jsxhint .",
    "npm-version": "npm install -g npm@`cat package.json | jq -r '.engines.npm'`",
    "size": "cat build/assets/bundle.*js | gzip-size | pretty-bytes && cat build/assets/bundle.*css | gzip-size | pretty-bytes",
    "start": "webpack-dev-server --colors --progress --hot --history-api-fallback --watch --port=3002 --host=0.0.0.0 --content-base public/",
    "test": "babel-tape-runner test/*.js | tap-spec"
  }
}
