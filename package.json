    "url": "http://lukekarrys.com"
  },
  "bugs": {
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com/issues"
  },
  "dependencies": {
    "alt": "^0.15.4",
    "bootstrap": "^3.3.4",
    "bootswatch": "^3.3.4",
    "bows": "^1.4.2",
    "bracket-data": "^2.3.0",
    "bracket-generator": "^2.0.4",
    "bracket-scorer": "^2.3.2",
    "bracket-updater": "^2.1.0",
    "bracket-validator": "^2.0.4",
    "classnames": "^1.2.0",
    "firebase": "^2.2.3",
    "lesshat": "^3.0.2",
    "load-script": "^1.0.0",
    "lodash": "^3.6.0",
    "qs": "^2.4.1",
    "raf": "^2.0.4",
    "react": "^0.13.1",
    "react-bootstrap": "^0.20.3",
    "react-router": "^0.13.2",
    "react-router-bootstrap": "^0.12.1",
    "react-timeago": "^2.0.2",
    "xhr": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.0.12",
    "babel-eslint": "^2.0.2",
    "babel-loader": "^5.0.0",
    "babel-tape-runner": "^1.1.0",
    "clean-css": "^3.1.9",
    "divshot-cli": "^1.10.4",
    "eslint": "^0.18.0",
    "eslint-plugin-react": "^2.1.0",
    "extract-text-webpack-plugin": "^0.5.0",
    "gzip-size": "^1.0.0",
    "html-webpack-plugin": "^1.1.0",
    "json-loader": "^0.5.1",
    "less": "^2.5.0",
    "less-import-inserter": "^1.0.1",
    "less-loader": "^2.2.0",
    "precommit-hook": "^2.0.1",
    "pretty-bytes": "^1.0.4",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.4",
    "style-loader": "^0.10.1",
    "tap-spec": "^3.0.0",
    "tape": "^4.0.0",
    "webpack": "^1.8.4",
    "webpack-dev-server": "^1.8.0"
  },
  "engines": {
    "node": "^0.10.35",
    "npm": "^2.7.0"
  },
  "homepage": "http://tweetyourbracket.com",
  "keywords": [
    "bracket",
    "ncaa",
    "tweetyourbracket"
  ],
  "license": "MIT",
  "main": "server.js",
  "pre-commit": [
    "lint",
    "test"
  ],
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com.git"
  },
  "scripts": {
    "build": "rm -rf build/ && NODE_ENV=production webpack && npm run build-after",
    "build-after": "npm run build-assets && npm run build-public && npm run build-cleancss",
    "build-assets": "mkdir -p build/assets/ && mv build/bundle.* build/assets",
    "build-cleancss": "cat build/assets/bundle.*css | cleancss -o `find build/assets/*.css -print0`",
    "build-public": "rsync -rv --exclude=*.html public/ build/",
    "build-static": "TYB_STATIC=true npm run build",
    "codeship-setup": "npm run npm-version && npm install && npm run build-static",
    "codeship-test": "npm test && npm run lint && npm run size",
    "deploy": "divshot",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx .",
    "npm-version": "npm install -g npm@`cat package.json | jq -r '.engines.npm'`",
    "size": "cat build/assets/bundle.*js | gzip-size | pretty-bytes && cat build/assets/bundle.*css | gzip-size | pretty-bytes",
    "start": "TYB_STATIC=true PORT=3002 webpack-dev-server --colors --progress --hot --history-api-fallback --watch --port=3002 --host=0.0.0.0 --content-base public/",
    "test": "babel-tape-runner test/*.js | tap-spec"
  }
}
