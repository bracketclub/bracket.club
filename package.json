{
  "name": "tweet-your-bracket",
  "version": "3.0.0",
  "author": {
    "name": "Luke Karrys",
    "email": "luke@lukekarrys.com",
    "url": "http://lukekarrys.com"
  },
  "bugs": {
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com/issues"
  },
  "dependencies": {
    "async": "~0.2.10",
    "backbone": "1.0.0",
    "bracket-generator": "~2.0.1",
    "bracket-scorer": "~1.0.2",
    "bracket-updater": "~2.0.0",
    "bracket-validator": "~2.0.1",
    "execSync": "~1.0.1-pre",
    "express": "3.x.x",
    "favicon-setter": "~0.1.1",
    "human-model": "~2.6.1",
    "human-view": "~1.6.3",
    "human-view-switcher": "~0.0.1",
    "jade": "~1.2.0",
    "jquery": "~2.1.0",
    "less": "~1.6.3",
    "mkdirp": "~0.3.5",
    "moonboots": "1.x.x",
    "rmrf": "~1.0.2",
    "simplewebrtc": "~1.1.1",
    "templatizer": "~0.2.10",
    "underscore": "~1.5.2"
  },
  "devDependencies": {
    "precommit-hook": "~0.3.8"
  },
  "homepage": "http://github.com/tweetyourbracket/tweetyourbracket.com",
  "jshintConfig": {
    "asi": false,
    "browser": true,
    "curly": false,
    "expr": true,
    "indent": 4,
    "loopfunc": true,
    "node": true,
    "predef": [
      "goinstant",
      "$",
      "app",
      "me"
    ],
    "trailing": true,
    "undef": true,
    "white": true
  },
  "keywords": [
    "ncaa",
    "bracket",
    "tweetyourbracket",
    "scores"
  ],
  "license": "MIT",
  "main": "server.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "http://github.com/tweetyourbracket/tweetyourbracket.com.git"
  },
  "scripts": {
    "local": "./node_modules/.bin/nodemon -e js,json,jade server.js",
    "build": "node server.js --build",
    "deploy": "npm run build && divshot push",
    "staging": "npm run build && divshot push staging",
    "production": "npm run build && divshot push production"
  }
}
