- if (complete)
    - var qs = "https://twitter.com/share?lang=en&related=tweetthebracket&via=tweetthebracket&count=none&hashtags=tybrkt&url=";
    - qs += encodeURIComponent("http://tweetyourbracket.com/" + url);
    - qs += "&text=" + encodeURIComponent(text);
    a.btn.btn-block.btn-primary.needsclick(href=qs, target="_blank") Enter My Bracket!
- else
    button.btn.btn-block.btn-primary.disabled(type="button", disabled="disabled") Make Your Picks!