{
  "name": "tweetyourbracket",
  "root": "./build",
  "clean_urls": true,
  "cache_control": {
    "/*.css": 31536000,
    "/*.js": 31536000,
    "/*.woff2": 31536000,
    "/*.svg": 31536000,
    "/*.ttf": 31536000,
